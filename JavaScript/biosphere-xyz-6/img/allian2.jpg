        <script>
        $(document).ready(function(){
            $(".captions-container").on('click', ".btn-accurate:not('.disabled')", function() {
                voteCaption(parseInt($(this).closest('.caption').attr('id')), 1);
            });
            $(".captions-container").on('click', ".btn-humor:not('.disabled')", function() {
                voteCaption(parseInt($(this).closest('.caption').attr('id')), 2);
            });
        });

        function voteCaption(caption_id, vote_value) {
            $('body').addClass('waiting');

            $.post("https://api.alphacoders.com/caption/vote", {
                                caption_id: caption_id,
                vote_value: vote_value
            },
            function(data) {
                if(data.hasOwnProperty('status') && data.status == "success") {
                    $("#" + caption_id + "_caption").find('.accurate-span').html(data.accurate);
                    $("#" + caption_id + "_caption").find('.humor-span').html(data.humor);

                    if(vote_value == 1) {
                        $("#" + caption_id + "_caption").find('.btn-accurate').addClass('disabled').attr('title', "Önceden derecelendirilmiş!");
                        $("#" + caption_id + "_caption").find('.btn-humor').removeClass('disabled').attr('title', "Bu başlığa 'Komik' olarak oy verin!")

                                            }
                    else if(vote_value == 2) {
                        $("#" + caption_id + "_caption").find('.btn-accurate').removeClass('disabled').attr('title', "Vote for this caption as 'Accurate' for this content!")
                        $("#" + caption_id + "_caption").find('.btn-humor').addClass('disabled').attr('title', "Önceden derecelendirilmiş!");

                                            }
                }
                else if(data.hasOwnProperty('error')) {
                    var reason = "Bu İşlemi Yapmaya İzniniz Yok!";

                    if(data.hasOwnProperty('error_message')) {
                        reason = data.error_message;
                    }
                    else if(data.error == "caption_not_found") {
                        reason = "The caption you are trying to vote for doesn't exist!";
                    }

                    handleApiError(reason);
                }
                $('body').removeClass('waiting');
            }, "json");
        }
    </script>

        <script>
        $('body').on('click', '.download-button',  function() {
            var element = $(this);

            $.post("https://api.alphacoders.com/content/get-download-link", {
                content_id: element.attr("data-id"),
                content_type: "wallpaper",
                file_type: element.attr("data-type"),
                image_server: element.attr("data-server")
            },
            function(data) {
                if(data.hasOwnProperty('status') && data.status == "success") {
                    element.attr("disabled", "disabled");

                    window.location.href = data.link;

                    setTimeout(function(){
                        element.removeAttr("disabled");
                    },5000);
                }
                else if(data.hasOwnProperty('error')) {
                    var reason = "The operation you attempted is not permitted!";

                    if(data.hasOwnProperty('error_message')) {
                        reason = data.error_message;
                    }

                    handleApiError(reason);
                }

            }, "json");
        });

        var nextPage = $("#next_page");
        var prevPage = $("#prev_page");
        prevUrl = prevPage.attr("href");
        nextUrl = nextPage.attr("href");

        document.onkeydown = function(evt) {
            evt = evt || window.event;
            var j = evt.keyCode;
            var x = evt.target||evt.srcElement;
            if(x != "[object HTMLInputElement]") {
                if (j == 39 && undefined != nextUrl) {
                    window.location = nextUrl;
                }
                else if(j == 37 && undefined != prevUrl) {
                    window.location = prevUrl;
                }
            }
        }
    </script>

    
