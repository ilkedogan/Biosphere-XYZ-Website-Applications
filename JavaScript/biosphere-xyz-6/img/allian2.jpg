    <script>
        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires="+d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";domain=.alphacoders.com;path=/;secure";
        }

        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for(var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }

        $(document).ready(function() {
            var cookie_status = getCookie("cookieconsent_status");
            if(cookie_status == "" || cookie_status == "deny") {
                $('#cookie_consent').css('display', 'flex');
            }

            $('#change_consent').on('click', function () {
                $('#cookie_consent').css('display', 'flex');
            });

            $('#cookie_consent .btn-allow').on('click', function () {
                setCookie('cookieconsent_status', 'allow', 365);
                $('#cookie_consent').css('display', 'none');
            });

            $('#cookie_consent .btn-deny').on('click', function () {
                setCookie('cookieconsent_status', 'deny', 365);
                window.location.href = "https://alphacoders.com/site/no-consent";
            });

            if($("#sorting_options").length) {
                $("#sorting_options").on('shown.bs.collapse', function() {
                    fixVideoAd('shown', $('#sorting_options').outerHeight() + 15);
                }).on('hide.bs.collapse', function() {
                    fixVideoAd('hidden', $('#sorting_options').outerHeight() + 15);
                });
            }
        });

        function fixVideoAd(action, height) {
            //Ad video has been detect
            if($('div[id^="clone_teaser_"]').length) {

                var ad_div = $('div[id^="teaser_"]');

                if(action == "shown") {
                    ad_div.css("top", parseInt(ad_div.css('top').replace(/[^-\d\.]/g, '')) + height);
                }
                else {
                    ad_div.css("top", parseInt(ad_div.css('top').replace(/[^-\d\.]/g, '')) - height);
                }
            }
        }

        function showWebsites() {
